$(".slider").slick({
  autoplay: true,
  autoplaySpeed: 3800,
  speed: 2000,
  adaptiveHeight: true,
  slidesToShow: 1,
  slidesToScroll: 1,
  responsive: true,
});

Vue({
  el: "#app",
  data: {
    slides: 5,
  },
  components: {
    "carousel-3d": Carousel3d.Carousel3d,
    slide: Carousel3d.Slide,
  },
});
